<section id="show-simulation-material" class="col-12">
  <div class="row">
    <%= render :partial => 'layouts/notice', :locals => { :notice => notice, :alert => alert } %>
  </div>
  <div class="row">
    <div class="col-12">
      <article class="card">
        <h3 class="card-header"><%=@simulation_material.simulation.title %> + <%=@simulation_material.material.title %></h3>
        <div class="card-body">
          <dl>
            <dt><%= t(:created_at, scope: [:activerecord, :attributes, :simulation_material]) %></dt>
            <dd><%= I18n.l @simulation_material.created_at.to_date, :format => :long %></dd>
            <dt><%= t(:updated_at, scope: [:activerecord, :attributes, :simulation_material]) %></dt>
            <dd><%= I18n.l @simulation_material.updated_at.to_date, :format => :long %></dd>
          </dl>
        </div>
      </article>
    </div>
  </div>
  <div class="row">
    <div class="col-12 col-xl-6">
      <%= link_to t(:link_list), admin_simulation_materials_path, class: 'btn btn-secondary' %>
    </div>
    <div class="col-12 col-xl-6 text-right">
      <%= link_to t(:edit), edit_admin_simulation_material_path(@simulation_material), class: 'btn btn-secondary' %>
      <%= link_to t(:delete), admin_simulation_material_path(@simulation_material), :method => :delete, :data => { :confirm => t(:delete_cascade_confirm) }, class: 'btn btn-danger' %>
    </div>
  </div>
</section>
