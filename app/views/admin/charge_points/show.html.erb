<section id="show-charge-point" class="col-12">
  <div class="row">
    <%= render :partial => 'layouts/notice', :locals => { :notice => notice, :alert => alert } %>
  </div>
  <div class="row">
    <div class="col-12">
      <div class="card">
        <h3 class="card-header"><%=@charge_point.title %></h3>
        <div class="card-body">
          <dl>
            <dt><%= t(:point, scope: [:activerecord, :attributes, :charge_point]) %></dt>
            <dd><%=@charge_point.point %></dd>
            <dt><%= t(:created_at, scope: [:activerecord, :attributes, :charge_point]) %></dt>
            <dd><%= I18n.l @charge_point.created_at.to_date, :format => :long %></dd>
            <dt><%= t(:updated_at, scope: [:activerecord, :attributes, :charge_point]) %></dt>
            <dd><%= I18n.l @charge_point.updated_at.to_date, :format => :long %></dd>
          </dl>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12 col-xl-6">
      <%= link_to t(:link_list), admin_charge_points_path, class: 'btn btn-secondary' %>
    </div>
    <div class="col-12 col-xl-6 text-right">
      <%= link_to t(:edit), edit_admin_charge_point_path(@charge_point), class: 'btn btn-secondary' %>
      <%= link_to t(:delete), admin_charge_point_path(@charge_point), :method => :delete, :data => { :confirm => t(:delete_confirm) }, class: 'btn btn-danger' %>
    </div>
  </div>
</section>
